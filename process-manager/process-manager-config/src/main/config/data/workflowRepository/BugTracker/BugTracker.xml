<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- DÉFINITION DE MODÈLE DE PROCÉDURE : SUIVI DE BUG -->
<processModel name="BugTracker" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="processModel.xsd">

	<label>Suivi de bugs</label>
	<description>Procédure de suivi de bugs pour Silverpeas.</description>

	<!-- DESCRIPTION DES ROLES -->
	<roles>
		<role name="Sender">
			<label>Emetteur</label>
			<label lang="en">Sender</label>
			<description>Personne pouvant emettre un bug.</description>
		</role>
		<role name="Qualificator">
			<label>Qualifieur</label>
			<label lang="en">Qualifyer</label>
			<description>Personne pouvant qualifier un bug.</description>
		</role>
		<role name="Prioritisator">
			<label>Prioritisateur</label>
			<label lang="en">Prioritisator</label>
			<description>Personne pouvant prioritiser un bug.</description>
		</role>
		<role name="Corrector">
			<label>Correcteur</label>
			<label lang="en">Corrector</label>
			<description>Personne pouvant corriger un bug.</description>
		</role>
		<role name="Validator">
			<label>Valideur</label>
			<label lang="en">Validator</label>
			<description>Personne pouvant valider la correction d'un bug.</description>
		</role>
		<role name="Viewer">
			<label>Lecteur</label>
			<label lang="en">Viewer</label>
			<description>Personne pouvant prioritiser un bug.</description>
		</role>
		<role name="Customer">
			<label>Client</label>
			<label lang="en">Customer</label>
			<description>Personne pouvant prioritiser un bug.</description>
		</role>
	</roles>

	<!-- DEFINITION DES DONNEES UTILISATEUR -->
	<userInfos>
		<item name="trigramme">
			<label>Votre trigramme</label>
			<label lang="en">Your initials</label>
			<type>text</type>
		</item>
	</userInfos>

	<!-- DEFINITION DU DOSSIER -->
	<dataFolder>
		<item name="bugId">
			<label>id</label>
			<description>Identifiant du bug</description>
			<type>text</type>
		</item>
		<item name="title">
			<label>Titre</label>
			<label lang="en">Title</label>
			<type>text</type>
		</item>
		<item name="description">
			<label>Description</label>
			<description>La description du bug</description>
			<type>text</type>
		</item>
		<item name="pbType">
			<label>Type de problème</label>
			<label lang="en">Problem type</label>
			<description>Indique si le type de pb (500, 404, NullPointer, Exception, Icone, AccessBD, Ergo)</description>
			<type>text</type>
			<param name="keys" value="Erreur 500##Erreur 404##NullPointerException##Icône##Accès à la base##Ergonomie##Autre"/>
		</item>
		<item name="client">
			<label>Client concerné</label>
			<label lang="en">Affected customer</label>
			<description>Indique chez quel client le bug a été rencontré</description>
			<type>text</type>
			<param name="keys" value="Interne##LRP##FT RD##FT OCISI##VSIC##Nature et Découvertes##Autre"/>
		</item>
		<item name="enteredBy">
			<label>Emetteur</label>
			<label lang="en">Sender</label>
			<description>Personne ayant ouvert le bug</description>
			<type>user</type>
		</item>
		<item name="qualifiedBy">
			<label>Qualifieur</label>
			<label lang="en">Qualifyer</label>
			<description>Personne devant qualifier le bug</description>
			<type>user</type>
		</item>
		<item name="product">
			<label>Produit, Version, dernier patch</label>
			<label lang="en">Product</label>
			<description>Le produit et sa version concernée, le dernier patch appliqué</description>
			<type>text</type>
		</item>
		<item name="platform">
			<label>Plateforme</label>
			<label lang="en">Platform</label>
			<description>La plateforme sous laquelle tourne Silverpeas</description>
			<type>text</type>
			<param name="keys" value="Windows NT##Solaris##Linux"/>
		</item>
		<item name="appServer">
			<label>Serveur d'application</label>
			<label lang="en">Application server</label>
			<description>Le serveur d'application sur lequel tourne Silverpeas</description>
			<type>text</type>
			<param name="keys" value="Orion 5.2##BEA Weblogic 6.1 SP1"/>
		</item>
		<item name="dbServer">
			<label>Serveur SGBD</label>
			<label lang="en">Database server</label>
			<description>Le serveur de base de données avec lequel fonctionne Silverpeas</description>
			<type>text</type>
			<param name="keys" value="MS SQL 2000##MS SQL 7##Oracle 8i##Oracle 9i"/>
		</item>
		<item name="reproducible">
			<label>Reproductible</label>
			<label lang="en">Reproducible</label>
			<description>Le bug a-t-il pu être reproduit</description>
			<type>text</type>
			<param name="keys" value="Systématiquement##De temps en temps##Rarement##Jamais"/>
		</item>
		<item name="stepsToReproduce">
			<label>Etapes pour reproduire le bug</label>
			<label lang="en">Steps to reproduce bug</label>
			<description>Les différentes étapes pour reproduire le bug</description>
			<type>text</type>
		</item>
		<item name="remarks">
			<label>Remarques</label>
			<label lang="en">Remarks</label>
			<description>Remarques éventuelles</description>
			<type>text</type>
		</item>
		<item name="proposedSolution">
			<label>Solution proposée</label>
			<label lang="en">Proposed solution</label>
			<description>Solution proposée</description>
			<type>text</type>
		</item>
		<item name="severity">
			<label>Gravité</label>
			<label lang="en">Gravity</label>
			<description>Gravité du bug</description>
			<type>text</type>
			<param name="keys" value="Contournement possible##Uniquement visuel##Limite les fonctionnalités##Plante le serveur"/>
		</item>
		<item name="priority">
			<label>Priorité</label>
			<label lang="en">Priority</label>
			<description>Priorité de correction du bug</description>
			<type>text</type>
			<param name="keys" value="Hyper bug##Haute##Moyenne##Basse"/>
		</item>
		<item name="assignedTo">
			<label>Assigné à</label>
			<label lang="en">Assigned to</label>
			<description>Personne chargée de la correction</description>
			<type>user</type>
		</item>
		<item name="plannedDate">
			<label>Date prévue de livraison</label>
			<label lang="en">Planned delivery date</label>
			<description>Date à laquelle le correctif devrait être livré</description>
			<type>date</type>
		</item>
		<item name="plannedCorrectionTime">
			<label>Temps prévu pour la correction</label>
			<label lang="en">Planned correction time</label>
			<description>Temps prévu pour la correction</description>
			<type>text</type>
		</item>
		<item name="correctionTime">
			<label>Temps passé pour la correction</label>
			<label lang="en">Effective correction time</label>
			<description>Temps passé pour la correction</description>
			<type>text</type>
		</item>
		<item name="workaround">
			<label>Correction du bug</label>
			<label lang="en">Bug correction</label>
			<description>Descriptif de la correction du bug</description>
			<type>text</type>
		</item>
		<item name="forwardTo">
			<label>Bug déjà rentré</label>
			<label lang="en">Bug already referenced</label>
			<description>Le bug a déjà fait l'objet d'une fiche de bug, dont on indique l'identifiant</description>
			<type>text</type>
		</item>
		<item name="modules">
			<label>Modules touchés, Tag</label>
			<label lang="en">Affected modules</label>
			<description>Liste des modules corrigés ainsi que le tag</description>
			<type>text</type>
		</item>
		<item name="patchCorrected">
			<label>Corrigé dans Version, Patch</label>
			<label lang="en">Corrected version</label>
			<description>Version/Patch dans laquelle la correction a eu lieu</description>
			<type>text</type>
		</item>
		<item name="reasonOfAbortion">
			<label>Raison</label>
			<label lang="en">Reason</label>
			<description>La raison de l'avortement du bug</description>
			<type>text</type>
			<param name="keys" value="Bug connu##Non corrigé##Sans objet"/>
		</item>
	</dataFolder>

	<!-- PRESENTATION DES PROCEDURES EN COURS-->
	<presentation>
		<title>${action.Creation.actor.trigramme} - ${folder.title}</title>
		<columns>
			<column item="bugId"/>
			<column item="client"/>
			<column item="priority"/>
			<column item="product"/>
			<column item="platform"/>
			<column item="appServer"/>
			<column item="dbServer"/>
		</columns>
	</presentation>

	<!-- DESCRIPTION DES PARTICIPANTS -->
	<participants>
		<participant name="pProjectManager" resolvedState="ToBePrioritized" role="Prioritisator">
			<label>Le chef de projet</label>
			<description>Le chef de projet qui a prioritisé le bug</description>
		</participant>
	</participants>

	<!-- DESCRIPTION DES ÉTATS -->
	<states>
		<!-- To Be Qualified -->
		<state name="ToBeQualified">
			<label>à qualifier</label>
			<label lang="en">To be qualified</label>
			<description>en cours de qualification</description>
			<workingUsers>
				<relatedUser folderItem="qualifiedBy" role="Qualificator"/>
			</workingUsers>
			<interestedUsers>
				<userInRole name="Viewer"/>
			</interestedUsers>
			<allowedActions>
				<allow action="Qualify"/>
				<allow action="NeedInfos"/>
				<allow action="Abort"/>
				<allow action="Delegate"/>
			</allowedActions>
		</state>

		<!-- To Be Qualified bis (pour la delegation)-->
		<state name="ToBeQualifiedBis">
			<label>à qualifier</label>
			<label lang="en">To be qualified</label>
			<description>en cours de qualification</description>
			<workingUsers>
				<relatedUser folderItem="qualifiedBy" role="Qualificator"/>
			</workingUsers>
			<interestedUsers>
				<userInRole name="Viewer"/>
			</interestedUsers>
			<allowedActions>
				<allow action="Qualify"/>
				<allow action="NeedInfos"/>
				<allow action="Abort"/>
				<allow action="DelegateBis"/>
			</allowedActions>
		</state>

		<!-- More infos needed -->
		<state name="MoreInfosNeeded">
			<label>plus d'infos nécessaires</label>
			<label lang="en">More infos needed</label>
			<description>Retourné à l'envoyeur pour qu'il donne plus d'infos</description>
			<workingUsers>
				<relatedUser folderItem="enteredBy" role="Sender"/>
			</workingUsers>
			<interestedUsers>
				<userInRole name="Viewer"/>
			</interestedUsers>
			<allowedActions>
				<allow action="SendInfos"/>
				<allow action="Abort"/>
			</allowedActions>
		</state>

		<!-- To Be Prioritized -->
		<state name="ToBePrioritized">
			<label>Qualifié</label>
			<label lang="en">Qualified</label>
			<description>à dispatcher</description>
			<workingUsers>
				<userInRole name="Prioritisator"/>
			</workingUsers>
			<interestedUsers>
				<userInRole name="Viewer"/>
			</interestedUsers>
			<allowedActions>
				<allow action="AssignCorrector"/>
				<allow action="NeedReQualification"/>
				<allow action="Abort"/>
			</allowedActions>
		</state>

		<!-- To Be Corrected -->
		<state name="ToBeCorrected">
			<label>Assigné</label>
			<label lang="en">Assigned</label>
			<description>En correction</description>
			<workingUsers role="Corrector">
				<relatedUser folderItem="assignedTo"/>
			</workingUsers>
			<interestedUsers>
				<userInRole name="Viewer"/>
			</interestedUsers>
			<allowedActions>
				<allow action="Correct"/>
				<allow action="NeedReQualification"/>
			</allowedActions>
		</state>

		<!-- To Be Validated -->
		<state name="ToBeValidated">
			<label>A valider</label>
			<label lang="en">To be validated</label>
			<description>En cours de validation</description>
			<workingUsers role="Validator">
				<relatedUser participant="pProjectManager"/>
			</workingUsers>
			<interestedUsers>
				<userInRole name="Viewer"/>
			</interestedUsers>
			<allowedActions>
				<allow action="Valid"/>
				<allow action="Refuse"/>
			</allowedActions>
		</state>

		<!-- Closed -->
		<state name="Closed">
			<label>Clos</label>
			<label lang="en">Closed</label>
			<description>Clos (résolu)</description>
			<workingUsers>
			</workingUsers>
			<interestedUsers>
				<userInRole name="Viewer"/>
			</interestedUsers>
			<allowedActions>
			</allowedActions>
		</state>

		<!-- Aborted -->
		<state name="Aborted">
			<label>Abandonné</label>
			<label lang="en">Aborted</label>
			<description>Abandonné</description>
			<workingUsers>
			</workingUsers>
			<interestedUsers>
				<userInRole name="Viewer"/>
			</interestedUsers>
			<allowedActions>
			</allowedActions>
		</state>
	</states>

	<!-- DESCRIPTION DES ACTIONS -->
	<actions>
		<!-- Creation -->
		<action name="Creation" form="fCreation" kind="create">
			<label>Création</label>
			<label lang="en">Creation</label>
			<description>Emission d'un nouveau bug.</description>
			<allowedUsers>
				<userInRole name="Sender"/>
			</allowedUsers>
			<consequences>
				<set state="ToBeQualified"/>
				<notify message="Vous devez qualifier le bug suivant : ${folder.title}">
					<relatedUser folderItem="qualifiedBy" role="Qualificator"/>
				</notify>
			</consequences>
		</action>

		<!-- Need more infos -->
		<action name="NeedInfos" form="fNeedInfos">
			<label>Demander plus d'infos</label>
			<label lang="en">Ask more infos</label>
			<description>Demander plus d'information à l'emetteur du bug.</description>
			<allowedUsers>
				<userInRole name="Qualificator"/>
			</allowedUsers>
			<consequences>
				<set state="MoreInfosNeeded"/>
				<notify message="Des précisions vous sont demandées sur le bug : ${folder.title}">
					<relatedUser folderItem="enteredBy" role="Sender"/>
				</notify>
			</consequences>
		</action>

		<!-- Need more infos -->
		<action name="SendInfos" form="fSendInfos">
			<label>Envoyer un complément d'infos</label>
			<label lang="en">Send more infos</label>
			<description>Envoyer un complément d'infos.</description>
			<allowedUsers>
				<userInRole name="Sender"/>
			</allowedUsers>
			<consequences>
				<set state="ToBeQualified"/>
				<notify message="Les précisions que vous avez demandées ont été rajoutées pour le bug : ${folder.title}">
					<relatedUser folderItem="qualifiedBy" role="Qualificator"/>
				</notify>
			</consequences>
		</action>

		<!-- Qualify -->
		<action name="Qualify" form="fQualify">
			<label>Qualifier le bug</label>
			<label lang="en">Qualify the bug</label>
			<description>Qualifier le bug.</description>
			<allowedUsers>
				<userInRole name="Qualificator"/>
			</allowedUsers>
			<consequences>
				<set state="ToBePrioritized"/>
				<notify message="Vous devez prioritiser le bug suivant : ${folder.title}">
					<userInRole name="Prioritisator"/>
				</notify>
			</consequences>
		</action>

		<!-- Delegate -->
		<action name="Delegate" form="fDelegate">
			<label>Déléguer le bug</label>
			<label lang="en">Delegate</label>
			<description>Déléguer le bug.</description>
			<allowedUsers>
				<userInRole name="Qualificator"/>
			</allowedUsers>
			<consequences>
				<set state="ToBeQualifiedBis"/>
				<notify message="Vous devez qualifier le bug suivant : ${folder.title}">
					<relatedUser folderItem="qualifiedBy" role="Qualificator"/>
				</notify>
			</consequences>
		</action>

		<!-- Delegate Bis-->
		<action name="DelegateBis" form="fDelegate">
			<label>Déléguer le bug</label>
			<label lang="en">Delegate</label>
			<description>Déléguer le bug.</description>
			<allowedUsers>
				<userInRole name="Qualificator"/>
			</allowedUsers>
			<consequences>
				<set state="ToBeQualified"/>
				<notify message="Vous devez qualifier le bug suivant : ${folder.title}">
					<relatedUser folderItem="qualifiedBy" role="Qualificator"/>
				</notify>
			</consequences>
		</action>

		<!-- Abort -->
		<action name="Abort" form="fAbort">
			<label>Abandonner le bug</label>
			<label lang="en">Abort the bug</label>
			<description>Abandonner le bug.</description>
			<allowedUsers>
				<userInRole name="Sender"/>
				<userInRole name="Qualificator"/>
				<userInRole name="Prioritisator"/>
			</allowedUsers>
			<consequences>
				<set state="Aborted"/>
			</consequences>
		</action>

		<!-- NeedReQualification -->
		<action name="NeedReQualification" form="fNeedReQualification">
			<label>Renvoyer au qualifieur</label>
			<label lang="en">Send back to qualifyer</label>
			<description>Retour pour qualification.</description>
			<allowedUsers>
				<userInRole name="Prioritisator"/>
				<userInRole name="Corrector"/>
			</allowedUsers>
			<consequences>
				<set state="ToBeQualified"/>
				<notify message="Vous devez requalifier le bug suivant : ${folder.title}">
					<relatedUser folderItem="qualifiedBy" role="Qualificator"/>
				</notify>
			</consequences>
		</action>

		<!-- AssignCorrector -->
		<action name="AssignCorrector" form="fAssignCorrector">
			<label>Choisir un correcteur</label>
			<label lang="en">Choose a corrector</label>
			<description>Choisir un correcteur.</description>
			<allowedUsers>
				<userInRole name="Prioritisator"/>
			</allowedUsers>
			<consequences>
				<set state="ToBeCorrected"/>
				<notify message="Vous devez corriger le bug suivant : ${folder.title} (id=${folder.bugId})">
					<relatedUser folderItem="assignedTo" role="Corrector"/>
				</notify>
			</consequences>
		</action>

		<!-- Correct -->
		<action name="Correct" form="fCorrect">
			<label>Envoyer le bug corrigé à la validation</label>
			<label lang="en">Send corrected version for validating</label>
			<description>Envoyer le bug corrigé à la validation.</description>
			<allowedUsers>
				<userInRole name="Corrector"/>
			</allowedUsers>
			<consequences>
				<set state="ToBeValidated"/>
				<notify message="Vous devez valider le bug suivant : ${folder.title} (id=${folder.bugId})">
					<relatedUser participant="pProjectManager"/>
				</notify>
			</consequences>
		</action>

		<!-- Valid -->
		<action name="Valid" form="fValid">
			<label>Valider la correction</label>
			<label lang="en">Validate correction</label>
			<description>Valider la correction.</description>
			<allowedUsers>
				<userInRole name="Validator"/>
			</allowedUsers>
			<consequences>
				<set state="Closed"/>
			</consequences>
		</action>

		<!-- Refuse -->
		<action name="Refuse" form="fRefuse">
			<label>Refuser la correction</label>
			<label lang="en">Refuse correction</label>
			<description>Refuser la correction.</description>
			<allowedUsers>
				<userInRole name="Validator"/>
			</allowedUsers>
			<consequences>
				<set state="ToBeQualified"/>
				<notify message="Le correction du bug a été refusée. Vous devez requalifier le bug suivant : ${folder.title} (id=${folder.bugId})">
					<relatedUser participant="pProjectManager"/>
				</notify>
			</consequences>
		</action>
	</actions>

	<!-- DEFINITION DES FORMULAIRES -->
	<forms>
		<!-- Formulaire de présentation -->
		<form name="presentationForm">
			<title>Fiche de bug</title>
			<title lang="en">Bug details</title>

			<input item="title"/>
			<input item="bugId"/>
			<input item="client"/>
			<input item="description" displayerName="textarea"/>
			<input item="pbType"/>
			<input item="enteredBy"/>
			<input item="qualifiedBy"/>
			<input item="product"/>
			<input item="platform"/>
			<input item="appServer"/>
			<input item="dbServer"/>
			<input item="reproducible"/>
			<input item="stepsToReproduce" displayerName="textarea"/>
			<input item="remarks" displayerName="textarea"/>
			<input item="severity"/>

			<input item="priority"/>
			<input item="assignedTo"/>
			<input item="plannedDate"/>

			<input item="plannedCorrectionTime"/>
			<input item="correctionTime"/>
			<input item="workaround" displayerName="textarea"/>
			<input item="forwardTo"/>
			<input item="modules" displayerName="textarea"/>
			<input item="patchCorrected"/>
			<input item="reasonOfAbortion"/>
		</form>

		<!-- Formulaire d'impression -->
		<form name="printForm" HTMLFileName="/weblib/processManager/BugTracker/BugTracker_print.html">
		</form>

		<!-- Formulaire de création -->
		<form name="fCreation">
			<title>Emission d'un bug</title>
			<title lang="en">Bug creation</title>
			<input item="title" mandatory="true"/>
			<input item="client" mandatory="true" displayerName="listbox"/>
			<input item="description" displayerName="textarea" mandatory="true"/>
			<input item="pbType" mandatory="true" displayerName="listbox"/>
			<input item="enteredBy" mandatory="true"/>
			<input item="qualifiedBy" mandatory="true"/>
			<input item="product" mandatory="true"/>
			<input item="platform" mandatory="true" displayerName="listbox"/>
			<input item="appServer" mandatory="true" displayerName="listbox"/>
			<input item="dbServer" mandatory="true" displayerName="listbox"/>
			<input item="reproducible" mandatory="true" displayerName="listbox"/>
			<input item="stepsToReproduce" displayerName="textarea" mandatory="true"/>
			<input item="remarks" displayerName="textarea" mandatory="true"/>
			<input item="severity" mandatory="true" displayerName="listbox"/>
		</form>

		<!-- Formulaire de demande d'infos complémentaire -->
		<form name="fNeedInfos">
			<title>Demande d'informations complémentaires</title>
			<title lang="en">Asking more information</title>
			<input item="remarks" displayerName="textarea" mandatory="true"/>
		</form>

		<!-- Formulaire d'envoi d'infos complémentaire -->
		<form name="fSendInfos">
			<title>Envoi d'informations complémentaires</title>
			<title lang="en">Sending more information</title>
			<input item="title" mandatory="true"/>
			<input item="client" readonly="true"/>
			<input item="description" displayerName="textarea" mandatory="true"/>
			<input item="pbType" mandatory="true" displayerName="listbox"/>
			<input item="enteredBy" mandatory="true"/>
			<input item="qualifiedBy" mandatory="true"/>
			<input item="product" mandatory="true"/>
			<input item="platform" mandatory="true" displayerName="listbox"/>
			<input item="appServer" mandatory="true" displayerName="listbox"/>
			<input item="dbServer" mandatory="true" displayerName="listbox"/>
			<input item="reproducible" mandatory="true" displayerName="listbox"/>
			<input item="stepsToReproduce" displayerName="textarea" mandatory="true"/>
			<input item="remarks" displayerName="textarea" mandatory="true"/>
			<input item="severity" mandatory="true" displayerName="listbox"/>
		</form>

		<!-- Formulaire de qualification -->
		<form name="fQualify">
			<title>Qualification du bug</title>
			<title lang="en">Bug qualification</title>
			<input item="client" readonly="true"/>
			<input item="description" displayerName="textarea" mandatory="true"/>
			<input item="pbType" mandatory="true" displayerName="listbox"/>
			<input item="reproducible" mandatory="true" displayerName="listbox"/>
			<input item="stepsToReproduce" displayerName="textarea" mandatory="true"/>
			<input item="remarks" displayerName="textarea" mandatory="true"/>
			<input item="proposedSolution" displayerName="textarea" mandatory="true"/>
			<input item="severity" mandatory="true" displayerName="listbox"/>

			<input item="priority" mandatory="true" displayerName="listbox"/>

			<input item="plannedCorrectionTime" mandatory="true"/>
		</form>

		<!-- Formulaire de delegation -->
		<form name="fDelegate">
			<title>Délégation du bug</title>
			<title lang="en">Bug delagation</title>
			<input item="qualifiedBy" mandatory="true"/>
		</form>

		<!-- Formulaire d'avortement -->
		<form name="fAbort">
			<title>Avorter le bug</title>
			<title lang="en">Bug abortion</title>
			<input item="reasonOfAbortion" mandatory="true" displayerName="listbox"/>
			<input item="forwardTo"/>
			<input item="remarks" displayerName="textarea" mandatory="true"/>
		</form>

		<!-- Formulaire de retour a la qualification -->
		<form name="fNeedReQualification">
			<title lang="en">Back to qualification</title>
			<input item="remarks" displayerName="textarea" mandatory="true"/>
		</form>

		<!-- Formulaire de prioritérisation -->
		<form name="fAssignCorrector">
			<title lang="en">Corrector assignation</title>
			<input item="bugId" mandatory="true"/>
			<input item="client" readonly="true"/>
			<input item="remarks" displayerName="textarea" mandatory="true"/>

			<input item="proposedSolution" displayerName="textarea" mandatory="true"/>
			<input item="priority" mandatory="true" displayerName="listbox"/>
			<input item="assignedTo" mandatory="true"/>
			<input item="plannedDate" mandatory="true"/>

			<input item="plannedCorrectionTime" mandatory="true"/>
		</form>

		<!-- Formulaire de correction -->
		<form name="fCorrect">
			<title lang="en">Correction</title>
			<input item="bugId" readonly="true"/>
			<input item="client" readonly="true"/>
			<input item="remarks" displayerName="textarea" mandatory="true"/>

			<input item="plannedCorrectionTime" mandatory="true" readonly="true"/>
			<input item="correctionTime" mandatory="true"/>
			<input item="workaround" mandatory="true" displayerName="textarea"/>
			<input item="modules" displayerName="textarea" mandatory="true"/>
		</form>

		<!-- Formulaire de validation -->
		<form name="fValid">
			<title lang="en">Validation</title>
			<input item="bugId" readonly="true"/>
			<input item="client" readonly="true"/>
			<input item="description" displayerName="textarea" mandatory="true"/>
			<input item="pbType" mandatory="true" displayerName="listbox"/>
			<input item="reproducible" mandatory="true" displayerName="listbox"/>
			<input item="stepsToReproduce" displayerName="textarea" mandatory="true"/>
			<input item="remarks" displayerName="textarea" mandatory="true"/>
			<input item="severity" mandatory="true" displayerName="listbox"/>

			<input item="priority" mandatory="true" displayerName="listbox"/>
			<input item="plannedDate" mandatory="true" readonly="true"/>

			<input item="patchCorrected" mandatory="true"/>
		</form>

		<!-- Formulaire de refus -->
		<form name="fRefuse">
			<title lang="en">Refusal</title>
			<input item="remarks" displayerName="textarea" mandatory="true"/>
		</form>
	</forms>
</processModel>
