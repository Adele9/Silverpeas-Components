<?xml version="1.0" encoding="ISO-8859-1"?>

<WAComponent>
  <name>processManager</name>
  <requestRouter>RprocessManager</requestRouter>
  <label>Processus 'Gestion des déplacements'</label>
  <description>Permet de gérer le processus de demande de déplacement et remboursement de la note de frais</description>
  <suite>05 Workflow</suite>
  <visible>@ComponentVisibleProcessManager@</visible>
   <portlet>no</portlet>
  <instanceClassName>com.silverpeas.processManager.ProcessManagerInstanciator</instanceClassName>
  <Profiles>
    <Profile name="Collaborateur">Demandeur</Profile>
    <Profile name="Responsable">Responsable</Profile>
    <Profile name="Secretaire">Secrétaire</Profile>
  </Profiles>
  <Parameters>
    <Parameter>
       <ParameterName>XMLFileName</ParameterName>
       <ParameterLabel>Modèle de procédures</ParameterLabel>
       <ParameterValue>DemandeDeMission/DemandeDeMission.xml</ParameterValue>
       <ParameterMandatory>Y</ParameterMandatory>
    </Parameter>
  </Parameters>

</WAComponent>
