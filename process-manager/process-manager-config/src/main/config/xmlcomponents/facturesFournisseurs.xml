    <Profile name="Controleurs_COM">Ass. COM</Profile>
    <Profile name="Controleurs_TEC">Ass. TEC</Profile>
-->
    <Profile name="Approbateur_RH">Resp. RH</Profile>
    <Profile name="Approbateur_CDG">Resp. CDG</Profile>
<!--
    <Profile name="Approbateur_TRA">Resp. TRA</Profile>
    <Profile name="Approbateur_LOG">Resp. LOG</Profile>
-->
    <Profile name="Approbateur_DSI">Resp. DSI</Profile>
    <Profile name="Approbateur_COM">Resp. COM</Profile>
    <Profile name="Approbateur_TEC">Resp. TEC</Profile>
    <Profile name="supervisor">Superviseur</Profile>
    <Profile name="Archiveurs">Archiveurs</Profile>
  </Profiles>

  <Parameters>
    <Parameter>
       <ParameterName>XMLFileName</ParameterName>
       <ParameterLabel>Modèle de procédures</ParameterLabel>
       <ParameterValue>facturesFournisseurs/facturesFournisseurs.xml</ParameterValue>
       <ParameterMandatory>Y</ParameterMandatory>
