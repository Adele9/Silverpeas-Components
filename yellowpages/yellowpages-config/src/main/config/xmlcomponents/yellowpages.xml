<?xml version="1.0" encoding="ISO-8859-1"?>

<WAComponent>
  <name>yellowpages</name>
  <label>Contacts - Annuaire Yellow pages</label>
  <description>Permet de créer un annuaire de contact. Un formulaire XML associé décrit l'identité de chaque membre de l'annuaire, ainsi que tout autre caractéristique.</description>
  <suite>02 Gestion Collaborative</suite>
  <visible>@ComponentVisibleYellowPages@</visible>
   <portlet>yes</portlet>
  <instanceClassName>com.stratelia.webactiv.yellowpages.YellowpagesInstanciator</instanceClassName>
  <tableList>
    <tabletpl>Node</tabletpl>
    <tabletpl>Contact</tabletpl>
    <tabletpl>ContactFather</tabletpl>
    <tabletpl>Model_contact</tabletpl>
    <tabletpl>Info</tabletpl>
    <tabletpl>InfoText</tabletpl>
    <tabletpl>InfoAttachment</tabletpl>
    <tabletpl>InfoImage</tabletpl>
    <tabletpl>InfoLink</tabletpl>
  </tableList>
<Profiles>
    <Profile name="admin">Gestionnaire</Profile>
    <Profile name="publisher">Publieur</Profile>
    <Profile name="user">Lecteur</Profile>
</Profiles>


</WAComponent>
